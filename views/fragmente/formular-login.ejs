<% if(!locals.utilizator) { %>
<form id="form_inreg" method="post" class="date" action="/login" enctype="multipart/form-data">
	<p>
		<label>
			<!-- Username:  -->
			<input class="form-control" type="text" name="username" id="inp-username" value="" required pattern="[A-Za-z0-9]+" placeholder="Username">
		</label>
	</p>
	<p>
		<label>
			<!-- Parola:  -->
			<input class="form-control" id="parl" type="password" required name="parola" value="" placeholder="Parola">
		</label>
	</p>
	<p><input class="btn btn-primary" type="submit" value="Logare"></p>
</form>
<% }  %>
<p><%- locals.mesajLogin %></p>